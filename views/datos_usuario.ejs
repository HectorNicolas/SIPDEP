<!DOCTYPE html>
<html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js" lang="">
<!--<![endif]-->

<head>
    <% include partials/head.ejs %>
</head>

<body>
    <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
            <![endif]-->
    <% include partials/appmenu.ejs %>

        <div class="container-fluid">
            <div class="container">
                <br>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h2 class="panel-title app-login-title">
                            Datos del usuario 
                        </h2>
                    </div>
                    <div class="panel-body">
                        <%if(typeof(usr_info) != 'undefined'){%>
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th></th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><strong>Nombre</strong></td>
                                        <td>
                                            <%=usr_info[0].Nombre + ' ' + usr_info[0].Apellidos%>
                                        </td>

                                    </tr>
                                    <tr>
                                        <td><strong>Correo</strong></td>
                                        <td>
                                            <%=usr_info[0].Correo%>
                                        </td>

                                    </tr>
                                    <tr>
                                        <td><strong>Sexo</strong></td>
                                        <td>
                                            <%=usr_info[0].Sexo%>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td> <strong>Fecha de nacimiento</strong></td>
                                        <td>
                                            <%=usr_info[0].FechaNacimiento%>
                                        </td>
                                    </tr>
                                    <%if(usr_info[0].TienePareja != null){%>
                                        <tr>
                                            <td> <strong>¿Tiene pareja sentimental?</strong></td>
                                            <td>
                                                <%=usr_info[0].TienePareja%>
                                            </td>
                                        </tr>
                                        <%}%>
                                            <%if(usr_info[0].TieneHijos != null){%>
                                                <tr>
                                                    <td><strong>¿Tiene hijos?</strong></td>
                                                    <td>
                                                        <%=usr_info[0].TieneHijos%>
                                                    </td>
                                                </tr>
                                                <%}%>
                                                    <%if(usr_info[0].ViveCon != null){%>
                                                        <tr>
                                                            <td><strong>¿Con quién vive?</strong></td>
                                                            <td>
                                                                <%=usr_info[0].ViveCon%>
                                                            </td>
                                                        </tr>
                                                        <%}%>
                                                            <%if(usr_info[0].DependeDe != null){%>
                                                                <tr>
                                                                    <td><strong>¿De quién depende económicamente?</strong></td>
                                                                    <td>
                                                                        <%=usr_info[0].DependeDe%>
                                                                    </td>
                                                                </tr>
                                                                <%}%>
                                                                    <%if(usr_info[0].ActividadFisica != null){%>
                                                                        <tr>
                                                                            <td><strong>¿Realiza alguna actividad física 3 o más veces por semana?</strong></td>
                                                                            <td>
                                                                                <%=usr_info[0].ActividadFisica%>
                                                                            </td>
                                                                        </tr>
                                                                        <%}%>
                                                                            <%if(usr_info[0].PosicionHijo != null){%>
                                                                                <tr>
                                                                                    <td><strong>Lugar de hijo que ocupa dentro de su familia</strong></td>
                                                                                    <td>
                                                                                        <%=usr_info[0].PosicionHijo%>
                                                                                    </td>
                                                                                </tr>
                                                                                <%}%>
                                                                                    <%if(usr_info[0].PadreMedico != null){%>
                                                                                        <tr>
                                                                                            <td><strong>¿Alguno de sus padres es médico?</strong></td>
                                                                                            <td>
                                                                                                <%=usr_info[0].PadreMedico%>
                                                                                            </td>
                                                                                        </tr>
                                                                                        <%}%>
                                                                                            <%if(usr_info[0].EscolPaterna != null){%>
                                                                                                <tr>
                                                                                                    <td><strong>Escolaridad paterna</strong></td>
                                                                                                    <td>
                                                                                                        <%=usr_info[0].EscolPaterna%>
                                                                                                    </td>
                                                                                                </tr>
                                                                                                <%}%>
                                                                                                    <%if(usr_info[0].EscolMaterna != null){%>
                                                                                                        <tr>
                                                                                                            <td><strong>Escolaridad materna</strong></td>
                                                                                                            <td>
                                                                                                                <%=usr_info[0].EscolMaterna%>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                        <%}%>
                                                                                                            <%if(usr_info[0].EsAlumno != null){%>
                                                                                                                <tr>
                                                                                                                    <td><strong>¿Es alumno de la UASLP?</strong></td>
                                                                                                                    <td>
                                                                                                                        <%=usr_info[0].EsAlumno%>
                                                                                                                    </td>
                                                                                                                </tr>
                                                                                                                <%}%>
                                                                                                                    <%}%>
                                                                                                                        <%if(typeof(alumno_info) != 'undefined'){%>
                                                                                                                            <tr>
                                                                                                                                <td><strong>Clave única</strong></td>
                                                                                                                                <td>
                                                                                                                                    <%=alumno_info[0].Clave_unica%>
                                                                                                                                </td>
                                                                                                                            </tr>
                                                                                                                            <tr>
                                                                                                                                <td><strong>Facultad</strong></td>
                                                                                                                                <td>
                                                                                                                                    <%=alumno_info[0].Facultad%>
                                                                                                                                </td>
                                                                                                                            </tr>
                                                                                                                            <tr>
                                                                                                                                <td><strong>Carrera</strong></td>
                                                                                                                                <td>
                                                                                                                                    <%=alumno_info[0].Carrera%>
                                                                                                                                </td>
                                                                                                                            </tr>
                                                                                                                            <tr>
                                                                                                                                <td><strong>Intentos de ingreso a la carrera</strong></td>
                                                                                                                                <td>
                                                                                                                                    <%=alumno_info[0].Intentos_Ingreso%>
                                                                                                                                </td>
                                                                                                                            </tr>
                                                                                                                            <tr>
                                                                                                                                <td><strong>Semestre en curso: </strong></td>
                                                                                                                                <td>
                                                                                                                                    <%=alumno_info[0].Semestre%>
                                                                                                                                </td>
                                                                                                                            </tr>
                                                                                                                            <%}%>
                                </tbody>
                            </table>
                    </div>
                </div>
            </div>

        </div>


        <% include partials/footer.ejs %>
            <script>
                window.jQuery || document.write('<script src="js/vendor/jquery-1.11.2.min.js"><\/script>')
            </script>
            <script src="js/vendor/bootstrap.min.js"></script>
            <script src="js/main.js"></script>
</body>

</html>